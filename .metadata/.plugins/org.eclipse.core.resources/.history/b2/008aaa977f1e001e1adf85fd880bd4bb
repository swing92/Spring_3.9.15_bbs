-- 전자문진 테이블
use theglory;

DROP TABLE IF EXISTS paper;
CREATE TABLE IF NOT EXISTS paper(
	id varchar(20) PRIMARY KEY,
	today VARCHAR(100),
    today_t VARCHAR(100),
    three VARCHAR(100),
    three_c VARCHAR(100),
    three_t VARCHAR(100),
    week VARCHAR(100),
    week_c VARCHAR(100),
    week_t VARCHAR(100),
    month VARCHAR(100),
    month_c VARCHAR(100),
    month_t VARCHAR(100),
    months VARCHAR(100),
    months_c VARCHAR(100),
    months_t VARCHAR(100),
    year VARCHAR(100),
    year_c VARCHAR(100),
    year_t VARCHAR(100),
    every VARCHAR(100),
    every_c VARCHAR(100),
    every_t VARCHAR(100),
    drug VARCHAR(150),
    drug_c VARCHAR(100),
    drug_t VARCHAR(100),
    infectious VARCHAR(100),
    infectious_t VARCHAR(100),
    malaria VARCHAR(100),
    malaria_t VARCHAR(100),
    vcjd VARCHAR(100),
    vcjd_t VARCHAR(100),
    reg_date TIMESTAMP NOT NULL
)ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

SELECT * FROM paper;